<h1>All Questions</h1>
<% @questions.each do |question| %>
<div class="media">
  <div class="media-body">
    <h4 class="media-heading">
      <p><%= link_to question.title, question_path(question.id) %></p>
      <% if question.resolved %>
        <p> Resolved </p>
      <% else %>
        <p> Pending </p>
      <% end %>
<% end %>
